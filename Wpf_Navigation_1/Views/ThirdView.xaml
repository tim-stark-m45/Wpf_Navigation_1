<UserControl x:Class="Wpf_Navigation_1.Views.ThirdView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Wpf_Navigation_1.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <Label Content="Third" FontSize="25"/>
        <Label Content="{Binding Message}" FontSize="25"/>
        <Button FontSize="25" Content="Alert" Command="{Binding AlertCommand}"/>
    </StackPanel>
</UserControl>
